{
  "name": "@my/s3",
  "version": "1.1.1-alpha.0",
  "description": "s3 wrapper",
  "main": ".dist/src/index.js",
  "typings": ".dist/src/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "npx rimraf .dist && npx tsc -b",
    "clean": "npx rimraf .dist",
    "clean:node": "npx rimraf node_modules",
    "build": "npx rimraf .dist && esbuild ./src/index.ts --bundle --sourcemap --platform=node --outdir=.dist",
    "postbuild": "npx tsc -b"
  },
  "keywords": [
    "s3"
  ],
  "author": "Iustin Burlacu",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.238.0",
    "@aws-sdk/s3-request-presigner": "^3.238.0",
    "@my/log": "^1.1.1-alpha.0"
  }
}
