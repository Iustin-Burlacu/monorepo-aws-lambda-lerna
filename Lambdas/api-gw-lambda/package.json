{
  "name": "api-gw-lambda",
  "version": "1.2.5-alpha.2",
  "description": "lambda triggered by api gateway",
  "main": ".dist/src/index.js",
  "scripts": {
    "test": "npm run test",
    "compile": "npx rimraf .dist && npx tsc -b",
    "deploy": "./deploy.sh",
    "clean": "npx rimraf .dist",
    "clean:node": "npx rimraf node_modules",
    "build": "npx rimraf .dist && esbuild ./src/index.ts --bundle --sourcemap --platform=node --outdir=.dist",
    "postbuild": "npx tsc -b"
  },
  "keywords": [
    "aws",
    "api-gateway",
    "lambda"
  ],
  "author": "Iustin Burlacu",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.284.0",
    "@aws-sdk/util-dynamodb": "^3.288.0",
    "@my/log": "^1.2.5-alpha.1",
    "aws-lambda": "^1.0.7",
    "ksuid": "^3.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/node": "^20.4.9",
    "ts-node": "^10.8.0",
    "typescript": "^4.9.4"
  }
}
